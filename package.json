{
    "name": "timings",
    "version": "1.1.1",
    "description": "RESTful API for performance engineering",
    "scripts": {
        "start": "node ./server --env prod",
        "start-local": "node ./server --env local",
        "start-local-debug": "node --debug ./server --env local",
        "start-dev": "node ./server --env dev",
        "start-dev-debug": "node --debug ./server --env dev",
        "start-test": "node ./server --env test",
        "start-test-debug": "node --debug ./server --env test",
        "test": "mocha --recursive --timeout 30000",
        "lint": "eslint-godaddy public/"
    },
    "engines": {
        "node": ">=8.0.0",
        "npm": ">=5.0.0"
    },
    "main": "server.js",
    "bin": {
        "timings": "./server.js"
    },
    "keywords": [
        "perf",
        "perfeng",
        "performance"
    ],
    "repository": "godaddy/timings",
    "author": "GoDaddy Operating Company, LLC",
    "contributors": [
        "Marcel Verkerk <mverkerk@godaddy.com>"
    ],
    "license": "MIT",
    "dependencies": {
        "body-parser": "^1.18.2",
        "cookie-parser": "~1.0.1",
        "elasticsearch": "~12.1.3",
        "express": "^4.16.2",
        "joi": "^11.4.0",
        "lucene-parser": "^0.1.2",
        "morgan": "~1.0.0",
        "nconf": "^0.9.1",
        "uuid": "^3.1.0",
        "winston": "^2.3.0",
        "yargs": "^8.0.2"
    },
    "devDependencies": {
        "chai": "^3.5.0",
        "chai-http": "^2.0.1",
        "eslint": "^4.10.0",
        "eslint-config-godaddy": "^2.0.0",
        "eslint-plugin-json": "^1.2.0",
        "eslint-plugin-mocha": "^4.11.0",
        "mocha": "^2.5.3"
    }
}
